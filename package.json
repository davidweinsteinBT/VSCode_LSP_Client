{
  "name": "vscode_client",
  "version": "1.0.0",
  "description": "",
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint ./src --ext .ts,.tsx"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/vscode": "^1.63.1",
    "eslint": "^8.7.0",
    "typescript": "^4.5.5",
    "vscode-languageclient": "^7.0.0"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "DavidLS.maxNumberOfProblems": {
            "type": "number",
            "default": 100
        },
        "DavidLS.trace.server": {
            "type": "string",
            "enum": [
                "off",
                "messages",
                "verbose"
            ],
            "default": "verbose"
        }
      }
    }
  }
}
